        "liquid_dpnrex": {
          "type": "liquid",
          "settings": {
            "liquid": "<!-- Bannière de garantie 30 jours -->\n{% comment %}\nVariables personnalisables (optionnelles)\n{% assign guarantee_days = '30' %}\n{% assign guarantee_text = 'JOURS SATISFAIT/REMBOURSÉ' %}\n{% endcomment %}\n\n{% assign guarantee_days = guarantee_days | default: '30' %}\n{% assign guarantee_text = guarantee_text | default: 'JOURS SATISFAIT/REMBOURSÉ' %}\n\n<div class=\"guarantee-banner\" style=\"\n  display: flex;\n  align-items: center;\n  background: linear-gradient(135deg, #dcfce7 0%, #bbf7d0 100%);\n  border: 2px solid #86efac;\n  border-radius: 12px;\n  padding: 16px 24px;\n  font-family: Arial, sans-serif;\n  max-width: 500px;\n  margin: 20px auto;\n  box-shadow: 0 2px 8px rgba(34, 197, 94, 0.1);\n\">\n  \n  <!-- Icône de validation -->\n  <div class=\"check-icon\" style=\"\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 32px;\n    height: 32px;\n    background-color: #16a34a;\n    border-radius: 50%;\n    margin-right: 16px;\n    flex-shrink: 0;\n  \">\n    <svg width=\"18\" height=\"18\" viewBox=\"0 0 24 24\" style=\"fill: white;\">\n      <path d=\"M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41L9 16.17z\"/>\n    </svg>\n  </div>\n  \n  <!-- Texte de garantie -->\n  <span style=\"\n    color: #166534;\n    font-size: 18px;\n    font-weight: bold;\n    letter-spacing: 0.5px;\n    text-transform: uppercase;\n  \">\n    {{ guarantee_days }} {{ guarantee_text }}\n  </span>\n</div>\n\n<!-- CSS responsive -->\n<style>\n  @media (max-width: 768px) {\n    .guarantee-banner {\n      padding: 12px 16px !important;\n      margin: 15px 10px !important;\n      max-width: calc(100% - 20px) !important;\n    }\n    \n    .guarantee-banner span {\n      font-size: 12px !important;\n      letter-spacing: 0.3px !important;\n    }\n    \n    .guarantee-banner .check-icon {\n      width: 20px !important;\n      height: 20px !important;\n      margin-right: 12px !important;\n    }\n    \n    .guarantee-banner svg {\n      width: 16px !important;\n      height: 16px !important;\n    }\n  }\n  \n  /* Animation au survol */\n  .guarantee-banner:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 4px 12px rgba(34, 197, 94, 0.2);\n    transition: all 0.3s ease;\n  }\n  \n  /* Animation de pulsation pour l'icône */\n  .guarantee-banner:hover .check-icon {\n    animation: pulse-green 1.5s infinite;\n  }\n  \n  @keyframes pulse-green {\n    0% { \n      transform: scale(1);\n      box-shadow: 0 0 0 0 rgba(22, 163, 74, 0.7);\n    }\n    70% {\n      transform: scale(1.05);\n      box-shadow: 0 0 0 10px rgba(22, 163, 74, 0);\n    }\n    100% {\n      transform: scale(1);\n      box-shadow: 0 0 0 0 rgba(22, 163, 74, 0);\n    }\n  }\n</style>"
          }
        },
        "liquid_xBhWW3": {
          "type": "liquid",
          "settings": {
            "liquid": "<!-- Alerte de stock personnalisée -->\n{% comment %}\nVariables personnalisables (optionnelles)\n{% assign stock_quantity = Très faible %}\n{% assign stock_text = 'Stock restant' %}\n{% assign remaining_text = 'restants' %}\n{% endcomment %}\n\n\n{% assign stock_text = stock_text | default: 'Stock restant' %}\n{% assign remaining_text = remaining_text | default: 'Très faible' %}\n\n<div class=\"stock-alert\" style=\"\n  display: inline-flex;\n  align-items: center;\n  background-color: #fef2f2;\n  border: 2px solid #dc2626;\n  border-radius: 12px;\n  padding: 12px 20px;\n  font-family: Arial, sans-serif;\n  margin: 10px 0;\n  max-width: fit-content;\n\">\n  \n  <!-- Icône d'avertissement -->\n  <div class=\"warning-icon\" style=\"\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 24px;\n    height: 24px;\n    background-color: #fbbf24;\n    border-radius: 4px;\n    margin-right: 10px;\n    flex-shrink: 0;\n  \">\n    <span style=\"\n      color: #000;\n      font-weight: bold;\n      font-size: 16px;\n      line-height: 1;\n    \">!</span>\n  </div>\n  \n  <!-- Texte du stock -->\n  <span style=\"\n    color: #dc2626;\n    font-size: 18px;\n    font-weight: bold;\n    white-space: nowrap;\n  \">\n    {{ stock_text }} : {{ stock_quantity }} {{ remaining_text }}\n  </span>\n</div>\n\n<!-- Version avec logique conditionnelle pour affichage automatique -->\n{% comment %}\nVersion avancée qui s'affiche automatiquement selon le stock\n{% endcomment %}\n\n{% if product.selected_or_first_available_variant.inventory_quantity %}\n  {% assign current_stock = product.selected_or_first_available_variant.inventory_quantity %}\n  \n  {% comment %} Afficher l'alerte seulement si le stock est faible (< 10 par exemple) {% endcomment %}\n  {% if current_stock > 0 and current_stock < 10 %}\n    <div class=\"stock-alert-auto\" style=\"\n      display: inline-flex;\n      align-items: center;\n      background-color: #fef2f2;\n      border: 2px solid #dc2626;\n      border-radius: 12px;\n      padding: 12px 20px;\n      font-family: Arial, sans-serif;\n      margin: 10px 0;\n      max-width: fit-content;\n    \">\n      \n      <!-- Icône d'avertissement -->\n      <div style=\"\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        width: 24px;\n        height: 24px;\n        background-color: #fbbf24;\n        border-radius: 4px;\n        margin-right: 10px;\n        flex-shrink: 0;\n      \">\n        <span style=\"\n          color: #000;\n          font-weight: bold;\n          font-size: 16px;\n          line-height: 1;\n        \">!</span>\n      </div>\n      \n      <!-- Texte du stock dynamique -->\n      <span style=\"\n        color: #dc2626;\n        font-size: 18px;\n        font-weight: bold;\n        white-space: nowrap;\n      \">\n        En stock : {{ current_stock }} \n        {% if current_stock == 1 %}\n          restant\n        {% else %}\n          restants\n        {% endif %}\n      </span>\n    </div>\n  {% endif %}\n{% endif %}\n\n<!-- CSS responsive -->\n<style>\n  @media (max-width: 768px) {\n    .stock-alert,\n    .stock-alert-auto {\n      padding: 10px 16px !important;\n      font-size: 16px !important;\n      margin: 8px 0 !important;\n    }\n    \n    .stock-alert span,\n    .stock-alert-auto span {\n      font-size: 16px !important;\n    }\n    \n    .warning-icon {\n      width: 20px !important;\n      height: 20px !important;\n      margin-right: 8px !important;\n    }\n    \n    .warning-icon span {\n      font-size: 14px !important;\n    }\n  }\n  \n  /* Animation d'apparition */\n  .stock-alert,\n  .stock-alert-auto {\n    animation: slideIn 0.3s ease-out;\n  }\n  \n  @keyframes slideIn {\n    from {\n      opacity: 0;\n      transform: translateY(-10px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n  \n  /* Effet de pulsation pour attirer l'attention */\n  .stock-alert:hover,\n  .stock-alert-auto:hover {\n    animation: pulse 1s infinite;\n  }\n  \n  @keyframes pulse {\n    0% { transform: scale(1); }\n    50% { transform: scale(1.02); }\n    100% { transform: scale(1); }\n  }\n</style>\n\n<!-- Version avec différents seuils de stock -->\n{% comment %}\nVersion avec couleurs différentes selon le niveau de stock\n{% endcomment %}\n\n{% if product.selected_or_first_available_variant.inventory_quantity %}\n  {% assign current_stock = product.selected_or_first_available_variant.inventory_quantity %}\n  \n  {% if current_stock > 0 and current_stock <= 20 %}\n    {% comment %} Définir les couleurs selon le niveau de stock {% endcomment %}\n    {% if current_stock <= 3 %}\n      {% assign alert_bg = '#fef2f2' %}\n      {% assign alert_border = '#dc2626' %}\n      {% assign alert_text_color = '#dc2626' %}\n      {% assign icon_bg = '#ef4444' %}\n      {% assign icon_text = '!' %}\n    {% elsif current_stock <= 10 %}\n      {% assign alert_bg = '#fefce8' %}\n      {% assign alert_border = '#eab308' %}\n      {% assign alert_text_color = '#ca8a04' %}\n      {% assign icon_bg = '#fbbf24' %}\n      {% assign icon_text = '!' %}\n    {% else %}\n      {% assign alert_bg = '#f0fdf4' %}\n      {% assign alert_border = '#22c55e' %}\n      {% assign alert_text_color = '#16a34a' %}\n      {% assign icon_bg = '#4ade80' %}\n      {% assign icon_text = '✓' %}\n    {% endif %}\n    \n    <div class=\"stock-alert-levels\" style=\"\n      display: inline-flex;\n      align-items: center;\n      background-color: {{ alert_bg }};\n      border: 2px solid {{ alert_border }};\n      border-radius: 12px;\n      padding: 12px 20px;\n      font-family: Arial, sans-serif;\n      margin: 10px 0;\n      max-width: fit-content;\n    \">\n      \n      <!-- Icône dynamique -->\n      <div style=\"\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        width: 24px;\n        height: 24px;\n        background-color: {{ icon_bg }};\n        border-radius: 4px;\n        margin-right: 10px;\n        flex-shrink: 0;\n      \">\n        <span style=\"\n          color: #fff;\n          font-weight: bold;\n          font-size: 14px;\n          line-height: 1;\n        \">{{ icon_text }}</span>\n      </div>\n      \n      <!-- Texte du stock avec couleur dynamique -->\n      <span style=\"\n        color: {{ alert_text_color }};\n        font-size: 18px;\n        font-weight: bold;\n        white-space: nowrap;\n      \">\n        En stock : {{ current_stock }} \n        {% if current_stock == 1 %}\n          restant\n        {% else %}\n          restants\n        {% endif %}\n      </span>\n    </div>\n  {% endif %}\n{% endif %}"
          }
